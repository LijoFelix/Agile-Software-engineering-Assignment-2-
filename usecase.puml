@startuml
actor Customer
actor Admin

rectangle "Online Bookstore" {
    Customer -- (Browse Books)
    Customer -- (Add to Cart)
    Customer -- (Checkout)
    Customer -- (Make Payment)

    Admin -- (Add New Book)
    Admin -- (Update Book Info)
    Admin -- (Remove Book)
}

(Browse Books) .> (Add to Cart) : include
(Checkout) .> (Make Payment) : include
@enduml
